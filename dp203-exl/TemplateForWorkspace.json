				"content": {
					"query": "create TABLE salescsv (\n    sale_id NVARCHAR(100),\n    product_id NVARCHAR(100),\n    product_name NVARCHAR(100),\n    quality int,\n    price float,\n    total_amount float, \n    sales_date DATE,\n    region NVARCHAR(100)\n\n)\n\n-- This is auto-generated code\nInsert into salesdatacsv(sale_id,product_id ,\n    product_name,\n    quality ,\n    price ,\n    total_amount , \n    sales_date ,\n    region)\nSELECT *\nFROM\n    OPENROWSET(\n        BULK 'https://datalake5hueysr.dfs.core.windows.net/files/sales_data/poscsv/**',\n        FORMAT = 'CSV',\n        PARSER_VERSION = '2.0',\n        HEADER_ROW = True\n    ) AS [result]\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "sql5hueysr",
						"poolName": "sql5hueysr"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
